<dockingLibrary:DockableContent x:Class="PorterApp.TypesTreeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dockingLibrary="clr-namespace:DockingLibrary;assembly=DockingLibrary"
        xmlns:viewModel="clr-namespace:PorterApp.ViewModel"
        xmlns:porterApp="clr-namespace:PorterApp"
        Title="Types Tree Window" Height="300" Width="300">
	<Window.Resources>
		<HierarchicalDataTemplate x:Key="ItemTemplate" DataType="viewModel:TypeTreeItem" ItemsSource="{Binding Path=Children}">
			<TextBlock Text ="{Binding Name}" porterApp:TextBlockBehavior.OnDoubleClick ="{Binding ToRemoveOnlyForTest}" porterApp:TextBlockBehavior.OnDoubleClickParam ="{Binding .}" />
		</HierarchicalDataTemplate>
	</Window.Resources>
	<porterApp:TreeView2 Margin="10,0,10,10"  ItemsSource="{Binding .}" ItemTemplate="{StaticResource ItemTemplate}"/>
    
</dockingLibrary:DockableContent>
